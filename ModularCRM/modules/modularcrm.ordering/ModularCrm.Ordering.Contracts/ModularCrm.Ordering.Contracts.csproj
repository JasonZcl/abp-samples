<Project Sdk="Microsoft.NET.Sdk">

  <Import Project="..\common.props" />

  <PropertyGroup>
    <TargetFramework>net10.0</TargetFramework>
    <Nullable>enable</Nullable>
    <RootNamespace>ModularCrm.Ordering</RootNamespace>
    <GenerateEmbeddedFilesManifest>true</GenerateEmbeddedFilesManifest>
    <CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Volo.Abp.Ddd.Application.Contracts" Version="10.0.0-rc.1" />
    <PackageReference Include="Volo.Abp.Authorization" Version="10.0.0-rc.1" />
    <PackageReference Include="Volo.Abp.Validation" Version="10.0.0-rc.1" />
  </ItemGroup>

  <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.FileProviders.Embedded" Version="10.0.0-rc.1.25451.107" />
  </ItemGroup>

  <ItemGroup>
      <EmbeddedResource Include="Localization\Ordering\*.json" />
      <Content Remove="Localization\Ordering\*.json" />
  </ItemGroup>

</Project>
